@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix core: <http://localhost:3001/ontologies/core/> .

@prefix dashboard1: <http://localhost:3001/ontologies/templates/dashboard1/> .

@prefix this: <http://localhost:3001/ontologies/template_instances/dashboard1_inst1/> .

this:instance
    a core:DashboardTemplateInstance ;
    core:instanceOf dashboard1:template ;
    core:dashboardTemplateInstanceContains this:arg_polygon_value .

this:arg_polygon_value
    a core:DashboardTemplateArgumentValue ;
    core:setsArgument dashboard1:arg_polygon ;
    core:value "[[0,0], [0,10], [50,10]]" .
